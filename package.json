{
  "name": "hoarder",
  "version": "0.0.0",
  "description": "Plugin Driven PVR Built in Node.js",
  "engineStrict": true,
  "engines": {
    "node": ">= 6.2.0"
  },
  "dependencies": {
    "inversify": "2.0.1",
    "later": "1.2.0",
    "sqlite3": "3.1.8",
    "typeorm": "0.0.2-alpha.69"
  },
  "devDependencies": {
    "@types/later": "0.0.28",
    "@types/node": "6.0.45",
    "@types/sequelize": "4.0.38",
    "@types/sqlite3": "^2.2.31",
    "del": "2.2.2",
    "gulp": "3.9.1",
    "gulp-nodemon": "2.2.1",
    "gulp-typescript": "3.0.2",
    "rimraf": "2.5.4",
    "typescript": "2.0.6"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish '{src,test}/**/*.{ts}'",
    "start": "npm run watch; node build/src/main"
  },
