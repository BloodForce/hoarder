    "type": "git",
    "url": "https://github.com/Shepless/hoarder.git"
  },
  "bugs": {
    "url": "https://github.com/Shepless/hoarder/issues"
  },
  "homepage": "https://github.com/Shepless/hoarder#readme"
}
